  },
  "devDependencies": {
    "@types/jest": "^27.5.2",
    "@types/puppeteer": "~7.0.4",
    "@types/node": "^16.18.11",
    "@types/ws": "8.5.6",
    "jest": "^27.5.1",
    "jest-cli": "^27.5.1",
    "puppeteer": "21.1.1",
    "rollup-plugin-node-polyfills": "^0.2.1",
    "@rollup/plugin-node-resolve": "^13.3.0"
